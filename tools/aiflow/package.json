{
  "name": "aiflow-local",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/cli.js",
  "bin": {
    "aiflow": "dist/cli.js"
  },
  "scripts": {
    "cli": "tsx src/cli.ts",
    "server": "tsx src/server/index.ts",
    "dev": "concurrently \"npm run server\" \"npm run ui:dev\"",
    "ui": "npm run ui:dev",
    "ui:dev": "vite --config ui/vite.config.ts",
    "ui:build": "vite build --config ui/vite.config.ts",
    "build": "tsup src/cli.ts --format esm,cjs --dts --sourcemap --out-dir dist",
    "doctor": "tsx src/cli.ts doctor"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "fast-glob": "^3.3.2",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "gray-matter": "^4.0.3",
    "marked": "^9.1.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.0.0",
    "@types/cors": "^2.8.17",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^8.2.2",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.6.3",
    "vite": "^5.0.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
